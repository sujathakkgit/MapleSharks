@if (@ViewBag.usrTyp == 1)
{
    <partial>Welcome @Model.Username ! You are logged in .. <br /> </partial>
    <a asp-controller="Home" asp-action="Index">Main page |</a>
    <a asp-controller="RegisterSwimmer" asp-action="ShowSwimmerData">Your registered swimmers account  | </a>
    <a asp-controller="Register" asp-action="AccountDetails">Account Details | </a>
    <a asp-controller="Login" asp-action="ProcessLogout"> Logout |</a>

}
else if (@ViewBag.usrTyp == 2)
{
    <partial> Hi @Model.Username, you are logged out <br />  </partial>
    <a asp-controller="Home" asp-action="Index">Main page |</a>
    <a asp-controller="Login" asp-action="Index">Login |</a>
}
else if (@ViewBag.usrTyp == 3)
{
    <partial> Hi @Model.Username,Check your login credentials,try logging in again with correct details <br />  </partial>
    <a asp-controller="Home" asp-action="Index">Main page |</a>
    <a asp-controller="Register" asp-action="AccountDetails">Account Details |</a>
    <a asp-controller="Login" asp-action="Index">Login |</a>

}
else
{
    <a asp-controller="Home" asp-action="Index">Main page |</a>
    <a asp-controller="Login" asp-action="Index">Login |</a>
    <a asp-controller="Register" asp-action="Index">Register |</a>
}

